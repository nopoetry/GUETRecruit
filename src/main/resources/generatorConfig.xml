        <!--                        <table tableName="tb_employment_information" domainObjectName="EmploymentInformation"/>-->
        <!--                <table tableName="tb_employment_way" domainObjectName="EmploymentWay"/>-->
        <!--                <table tableName="tb_organization_num" domainObjectName="OrganizationNum"/>-->
        <table tableName="tb_person_info" domainObjectName="PersonInfo"/>
